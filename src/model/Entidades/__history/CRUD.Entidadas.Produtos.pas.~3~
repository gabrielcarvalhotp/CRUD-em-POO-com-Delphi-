unit CRUD.Entidadas.Produtos;

interface

type
  TProdutos = class
  private
    Fdescricao: integer;
    Fpreco: integer;
    Fdata_cadastro: Tdate;
    Fid: integer;
    Fnome: integer;
  public
    constructor create;
    destructor destroy; override;
    property id : integer read Fid write Fid;
    property nome : integer read Fnome write Fnome;
    property preco : integer read Fpreco write Fpreco;
    property data_cadastro : Tdate read Fdata_cadastro write Fdata_cadastro;
    property descricao : integer read Fdescricao write Fdescricao;
    
  end;

implementation

{ TOrcamento_detalhe }

constructor TProdutos.create;
begin

end;

destructor TProdutos.destroy;
begin

  inherited;
end;

end.
